{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es5",                                
    "lib": ["DOM", "DOM.Iterable", "ESNext"],                                
    "module": "esnext",                             
    "moduleResolution": "node",                     
    "allowJs": true,                              
    "isolatedModules": true,                      
    "allowSyntheticDefaultImports": true,      
    "esModuleInterop": true,                          
    "forceConsistentCasingInFileNames": true,        
    "strict": true,                                      
      "noFallthroughCasesInSwitch": true,              
    "skipLibCheck": true                               
  },
  "include": ["src"]
}
